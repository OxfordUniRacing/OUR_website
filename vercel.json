{
  "redirects": [
    {
      "source": "/team-members",
      "destination": "/team",
      "permanent": true
    },
    {
      "source": "/team-members/:path*",
      "destination": "/team",
      "permanent": true
    },
    {
      "source": "/blog",
      "destination": "/news",
      "permanent": true
    },
    {
      "source": "/blog/:slug",
      "destination": "/news/:slug",
      "permanent": true
    },
    {
      "source": "/partners",
      "destination": "/sponsors",
      "permanent": true
    },
    {
      "source": "/sponsors/:slug",
      "destination": "/sponsors",
      "permanent": true
    },
    {
      "source": "/about-us",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/contact-us",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/formula-student",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/competitions",
      "destination": "/news",
      "permanent": true
    },
    {
      "source": "/gallery",
      "destination": "/news",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ]
}